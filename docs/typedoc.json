{
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-zod"],
  "gitRevision": "main",

  "fileExtension": ".mdx",
  "useCodeBlocks": true,
  "expandParameters": false,
  // must be true to show details of IndexerAPICommentSchemaType.results[]
  "expandObjects": true,
  "indexFormat": "table",
  "typeDeclarationVisibility": "verbose",
  "outputFileStrategy": "members",
  "entryFileName": "index.mdx",
  "mergeReadme": true
}
