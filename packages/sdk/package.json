{
  "name": "@ecp.eth/sdk",
  "version": "0.0.13",
  "type": "module",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "typings": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.cjs"
    },
    "./abis": {
      "types": "./dist/esm/abis.d.ts",
      "import": "./dist/esm/abis.js",
      "default": "./dist/cjs/abis.cjs"
    },
    "./react": {
      "types": "./dist/esm/react.d.ts",
      "import": "./dist/esm/react.js",
      "default": "./dist/cjs/react.cjs"
    },
    "./schemas": {
      "types": "./dist/esm/schemas.d.ts",
      "import": "./dist/esm/schemas.js",
      "default": "./dist/cjs/schemas.cjs"
    },
    "./types": {
      "types": "./dist/esm/types.d.ts",
      "import": "./dist/esm/types.js",
      "default": "./dist/cjs/types.cjs"
    }
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "dev": "tsup --format esm --out-dir dist/esm --watch",
    "build:cjs": "tsup --format cjs --out-dir dist/cjs",
    "build:esm": "tsup --format esm --out-dir dist/esm",
    "build": "rm -rf dist && npm run build:cjs && npm run build:esm",
    "test": "echo 'Add test script here'",
    "lint": "eslint \"./src/**/*.ts\""
  },
  "devDependencies": {
    "@ecp.eth/eslint-config": "workspace:*",
    "@ecp.eth/typescript-config": "workspace:*",
    "@tanstack/react-query": ">=5.0.0",
    "@types/react": "^19.0.8",
    "react": "18 || 19",
    "viem": "^2.22.21",
    "wagmi": "^2.14.9",
    "zod": "^3.24.1"
  },
  "dependencies": {
    "effect": "^3.12.9",
    "lz-ts": "^1.1.2"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "react": "18 || 19",
    "viem": "^2.22.21",
    "wagmi": "^2.14.9",
    "zod": "^3.24.1"
  }
}
