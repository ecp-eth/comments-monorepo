{
  "name": "@ecp.eth/protocol",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build:abi": "node ./build-abi.js",
    "test": "forge test -vvv",
    "debug:gas": "forge script script/debug.s.sol:DebugGasUsage",
    "node": "anvil --block-time 2",
    "dev": "node dev.js",
    "deploy:dev": "forge script script/deploy.s.sol:DeployScript --sig \"run(string)\" \"dev\" --rpc-url http://localhost:8545 --broadcast --force",
    "deploy:test": "forge script script/deploy.s.sol:DeployScript --sig \"run(string)\" \"test\" --rpc-url http://localhost:8545 --broadcast --force",
    "deploy": " forge script script/deploy.s.sol:DeployScript --sig \"run(string)\" \"prod\" --rpc-url http://localhost:8545 --broadcast"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.3.0",
    "forge-std": "github:foundry-rs/forge-std#v1.9.6",
    "solady": "^0.1.14"
  },
