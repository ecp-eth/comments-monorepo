{
  "name": "@ecp.eth/indexer",
  "version": "0.0.23",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22 <23"
  },
  "scripts": {
    "predev": "pnpm db:migrate:up",
    "dev": "ponder dev",
    "prestart": "NODE_ENV=production pnpm db:migrate:up",
    "start": "NODE_ENV=production ponder start",
    "start:supervisor": "node --experimental-transform-types ./bin/supervisor.ts",
    "db": "ponder db",
    "codegen": "ponder codegen",
    "serve": "ponder serve",
    "lint": "eslint \"./src/**/*.ts\"",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "drizzle-kit": "dotenvx run --convention nextjs -- drizzle-kit",
    "db:migrate:generate": "pnpm drizzle-kit generate",
    "db:migrate:up": "pnpm drizzle-kit migrate",
    "worker:fan-out": "dotenvx run --convention nextjs -- node --experimental-transform-types ./bin/event-fan-out-worker.ts",
    "worker:webhook-event-delivery": "dotenvx run --convention nextjs -- node --experimental-transform-types ./bin/webhook-event-delivery-worker.ts"
  },
  "dependencies": {
    "@0x/0x-parser": "2.10.3",
    "@dotenvx/dotenvx": "^1.48.4",
    "@ecp.eth/sdk": "workspace:*",
    "@ecp.eth/shared": "workspace:*",
    "@envelop/sentry": "^14.0.0",
    "@grammyjs/conversations": "^2.1.0",
    "@grammyjs/menu": "^1.3.0",
    "@grammyjs/parse-mode": "^2.2.0",
    "@hono/node-server": "^1.18.2",
    "@hono/zod-openapi": "^1.1.0",
    "@neynar/nodejs-sdk": "^2.44.0",
    "@noble/ed25519": "^2.2.3",
    "@ponder/client": "0.12.21",
    "@sentry/node": "^9.3.0",
    "@sentry/profiling-node": "^9.3.0",
    "@sentry/tracing": "^7.120.3",
    "axios": "^1.11.0",
    "commander": "^13.1.0",
    "dataloader": "^2.2.3",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "0.41.0",
    "grammy": "^1.37.0",
    "graphql-request": "^7.2.0",
    "hono": "^4.9.1",
    "hono-rate-limiter": "^0.4.2",
    "is-network-error": "^1.1.0",
    "lru-cache": "^11.0.2",
    "node-html-parser": "^7.0.1",
    "normalize-url": "^8.0.1",
    "pg": "^8.11.3",
    "ponder": "0.12.21",
    "promise-deferred": "^2.0.4",
    "telegraf": "^4.16.3",
    "uuid": "^11.1.0",
    "viem": "^2.29.2",
    "xxhash-wasm": "^1.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@ecp.eth/eslint-config": "workspace:*",
    "@types/node": "^22.13.1",
    "@types/promise-deferred": "^2.0.5",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "nock": "^14.0.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  }
}
